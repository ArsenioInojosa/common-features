{
  "compileOnSave": true,
  "compilerOptions": {
    "allowJs": false,
    "baseUrl": ".",
    "declaration": true,
    "emitBOM": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "inlineSources": true,
    "jsx": "react",
    "moduleResolution": "Node",
    "noEmit": false,
    "noEmitHelpers": true,
    "noEmitOnError": false,
    "outDir": "./wwwroot",
    "paths": {
      "@serenity-is/*": [ "node_modules/@serenity-is/*/dist/index" ]
    },
    "preserveConstEnums": true,
    "skipDefaultLibCheck": true,
    "sourceMap": true,
    "target": "es6"
  },
  "typeAcquisition": {
    "enable": false
  }
}
